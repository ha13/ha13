<h2>List Article</h2>
<table >
  <tr>
    <th>Title</th>
    <th>Text</th>
  </tr>
  <% @article.each{|a|%>
    <tr>
      <td>
        <%= a.title%>
      </td>
      <td>
        <%= a.string%>
      </td>
      <td>
        <%= link_to 'Show', a %>
      </td>
       <td>
        <%= link_to 'Preview', article_preview_path(a) %>
      </td>
      <td>
        <%= link_to 'Edit', a %>
      </td>
      <td>
        <%= link_to 'Delete', a, method: :delete,data: {confirm: "Are u sure ?"}%>
      </td>
    </tr>
  <%}%>

</table>
<%= link_to 'New Article', new_article_url %></br>
<%= link_to 'New1 Article' , new1_new_article_path %></br>
